{"ast":null,"code":"export default {\n  name: 'NavBar',\n  data() {\n    return {\n      activeSection: '',\n      sections: [{\n        id: 'sobre',\n        name: 'Sobre'\n      }, {\n        id: 'habilidades',\n        name: 'Habilidades'\n      }, {\n        id: 'portifolio',\n        name: 'Portifólio'\n      }, {\n        id: 'experiencia',\n        name: 'Experiências'\n      }, {\n        id: 'certificado',\n        name: 'Certificados'\n      }, {\n        id: 'contato',\n        name: 'Contato'\n      }]\n    };\n  },\n  mounted() {\n    window.addEventListener('scroll', this.onScroll);\n  },\n  beforeUnmount() {\n    window.removeEventListener('scroll', this.onScroll);\n  },\n  methods: {\n    setActiveSection(sectionId) {\n      const element = document.getElementById(sectionId);\n      if (element) {\n        element.scrollIntoView({\n          behavior: 'smooth'\n        });\n        setTimeout(() => {\n          window.scrollBy(0, -100); // Ajuste esse valor conforme necessário\n        }, 300);\n      }\n      this.activeSection = sectionId;\n    }\n  }\n};","map":{"version":3,"names":["name","data","activeSection","sections","id","mounted","window","addEventListener","onScroll","beforeUnmount","removeEventListener","methods","setActiveSection","sectionId","element","document","getElementById","scrollIntoView","behavior","setTimeout","scrollBy"],"sources":["C:\\Users\\douglas.souza\\Desktop\\curriculo\\src\\components\\NavBar.vue"],"sourcesContent":["<template>\n    <header>\n        <nav class=\"navbar navbar-expand-lg navbar-dark bg-black\">\n            <div class=\"container\">\n                <ul class=\"navbar-nav me-auto mb-2 mb-lg-0\">\n                    <li class=\"nav-item\" v-for=\"section in sections\" :key=\"section.id\">\n                        <a class=\"nav-link\"\n                           :href=\"`#${section.id}`\"\n                           :class=\"{ active: activeSection === section.id }\"\n                           @click
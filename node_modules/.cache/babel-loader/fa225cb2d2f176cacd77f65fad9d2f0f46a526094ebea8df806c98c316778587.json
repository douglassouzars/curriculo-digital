{"ast":null,"code":"import { Bar } from 'vue-chartjs';\nimport { Chart as ChartJS, CategoryScale, LinearScale, BarElement, Title, Tooltip, Legend } from 'chart.js';\nChartJS.register(CategoryScale, LinearScale, BarElement, Title, Tooltip, Legend);\nexport default {\n  name: 'HabilidadesSection',\n  components: {\n    Bar\n  },\n  data() {\n    return {\n      data: {\n        labels: ['@/assets/habilidades/python.png', '@/assets/habilidades/c.png', '@/assets/habilidades/docker.png', '@/assets/habilidades/js.png', '@/assets/habilidades/vue.png', '@/assets/habilidades/ad.png', '@/assets/habilidades/virtualbox.png', '@/assets/habilidades/ubuntu.png', '@/assets/habilidades/django.png', '@/assets/habilidades/mysql.png', '@/assets/habilidades/postegresql.png', '@/assets/habilidades/gitlab.png', '@/assets/habilidades/sqlserver.png', '@/assets/habilidades/html.png', '@/assets/habilidades/aspnet.png'],\n        datasets: [{\n          label: 'Proficiency Level',\n          data: [90, 60, 30, 80, 45, 85, 80, 70, 85, 60, 50, 70, 70, 90, 75],\n          backgroundColor: ['rgba(255, 99, 132, 0.6)', 'rgba(54, 162, 235, 0.6)', 'rgba(255, 206, 86, 0.6)', 'rgba(75, 192, 192, 0.6)', 'rgba(153, 102, 255, 0.6)', 'rgba(255, 159, 64, 0.6)', 'rgba(199, 199, 199, 0.6)', 'rgba(83, 102, 255, 0.6)', 'rgba(243, 156, 18, 0.6)', 'rgba(39, 174, 96, 0.6)', 'rgba(192, 57, 43, 0.6)', 'rgba(44, 62, 80, 0.6)', 'rgba(127, 140, 141, 0.6)', 'rgba(241, 196, 15, 0.6)', 'rgba(230, 126, 34, 0.6)'],\n          borderColor: 'rgba(0, 0, 0, 0.1)',\n          borderWidth: 1,\n          barThickness: 20\n        }]\n      },\n      options: {\n        responsive: true,\n        plugins: {\n          legend: {\n            display: true,\n            position: 'top'\n          },\n          tooltip: {\n            callbacks: {\n              label: function (tooltipItem) {\n                return `${tooltipItem.raw}%`;\n              }\n            }\n          }\n        },\n        scales: {\n          y: {\n    
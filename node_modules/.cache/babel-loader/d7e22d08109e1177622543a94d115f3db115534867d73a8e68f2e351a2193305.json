is.currentImage = '';  // Limpa a imagem atual\n        }\n    }\n};\n</script>\n\n<style scoped>\nsection {\n    background-color: darkgray;\n    padding: 20px;\n    height: 800px;\n}\n\n.gallery {\n    display: flex;\n    flex-wrap: wrap;\n    gap: 10px;\n}\n\n.image-item {\n    flex: 1 1 calc(25% - 15px); /* Ajusta a largura para 4 imagens por linha, considerando o espaço entre elas */\n    box-sizing: border-box;\n}\n\n.img-thumbnail {\n    width: 100%;\n    height: 300px;\n    border: 1px solid #ddd;\n    cursor: pointer;\n    transition: transform 0.3s ease;\n}\n\n.img-thumbnail:hover {\n    transform: scale(1.05);  /* Efeito de zoom nas imagens */\n}\n\n/* Estilo para o modal */\n.modal {\n    position: fixed;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    background-color: rgba(0, 0, 0, 0.7);\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    z-index: 9999;\n}\n\n.modal-content {\n    position: relative;\n    max-width: 80%;\n    max-height: 80%;\n}\n\n.modal-image {\n    width: 100%;\n    height: auto;\n    border-radius: 8px;\n}\n\n.modal:hover {\n    cursor: pointer;\n}\n</style>"],"mappings":"AAqBA,eAAe;EACXA,IAAI,EAAE,oBAAoB;EAC1BC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE,KAAK;MAAG;MACnBC,YAAY,EAAE,EAAE;MAAG;MACnBC,MAAM,EAAE,CACJ;QAAEC,EAAE,EAAE,CAAC;QAAEC,GAAG,EAAEC,OAAO,CAAC,mCAAmC,CAAC;QAAEC,GAAG,EAAE;MAAgB,CAAC,EAClF;QAAEH,EAAE,EAAE,CAAC;QAAEC,GAAG,EAAEC,OAAO,CAAC,kCAAkC,CAAC;QAAEC,GAAG,EAAE;MAAgB,CAAC,EACjF;QAAEH,EAAE,EAAE,CAAC;QAAEC,GAAG,EAAEC,OAAO,CAAC,mCAAmC,CAAC;QAAEC,GAAG,EAAE;MAAgB,CAAC,EAClF;QAAEH,EAAE,EAAE,CAAC;QAAEC,GAAG,EAAEC,OAAO,CAAC,iCAAiC,CAAC;QAAEC,GAAG,EAAE;MAAgB,CAAC,EAChF;QAAEH,EAAE,EAAE,CAAC;QAAEC,GAAG,EAAEC,OAAO,CAAC,iCAAiC,CAAC;QAAEC,GAAG,EAAE;MAAgB,CAAC,EAChF;QAAEH,EAAE,EAAE,CAAC;QAAEC,GAAG,EAAEC,OAAO,CAAC,mCAAmC,CAAC;QAAEC,GAAG,EAAE;MAAgB,CAAC,EAClF;QAAEH,EAAE,EAAE,CAAC;QAAEC,GAAG,EAAEC,OAAO,CAAC,mCAAmC,CAAC;QAAEC,GAAG,EAAE;MAAgB,CAAC,EAClF;QAAEH,EAAE,EAAE,CAAC;QAAEC,GAAG,EAAEC,O